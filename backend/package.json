{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "node -e \"require('fs').cpSync('src/db/migrations', 'dist/db/migrations', {recursive: true})\"",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:rollback": "tsx src/db/rollback.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "axios": "1.4.0",
    "cheerio": "1.0.0-rc.10",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "fast-check": "^3.15.0"
  }
}
